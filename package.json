{
	"name": "@lusc/sudoku",
	"version": "1.3.1",
	"repository": "https://github.com/melusc/sudoku",
	"author": "lusc",
	"license": "MIT",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist/lib"
	],
	"engines": {
		"node": "^12.20.0 || ^14.13.1 || >=16.0.0"
	},
	"main": "./dist/lib/index.js",
	"type": "module",
	"ava": {
		"files": [
			"test/**/*.test.ts"
		],
		"environmentVariables": {
			"NODE_ENV": "test"
		},
		"typescript": {
			"compile": false,
			"rewritePaths": {
				"test/": "dist/test/test/"
			}
		}
	},
	"scripts": {
		"prepublishOnly": "yarn run build",
		"fmt": "prettier -w . && xo --fix",
		"build": "del-cli ./dist/lib/** && tsc",
		"test": "yarn run ava",
		"pretest": "del-cli ./dist/test/** && tsc --project test/tsconfig.json"
	},
	"devDependencies": {
		"@ava/typescript": "^3.0.1",
		"@lusc/tsconfig": "^1.0.0",
		"@types/node": "^17.0.10",
		"ava": "^4.0.1",
		"del-cli": "^4.0.1",
		"prettier": "^2.5.1",
		"typescript": "^4.5.5",
		"xo": "^0.47.0"
	}
}
