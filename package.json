{
	"name": "@lusc/sudoku",
	"version": "4.0.0",
	"repository": "https://github.com/melusc/sudoku",
	"author": "lusc",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist/src"
	],
	"engines": {
		"node": "^16.9 || ^18 || ^20"
	},
	"exports": {
		".": "./dist/src/index.js",
		"./plugins": "./dist/src/plugins/index.js"
	},
	"type": "module",
	"ava": {
		"files": [
			"test/**/*.test.ts"
		],
		"environmentVariables": {
			"NODE_ENV": "test"
		},
		"typescript": {
			"compile": false,
			"rewritePaths": {
				"test/": "dist/test/"
			}
		}
	},
	"scripts": {
		"prepublishOnly": "yarn run build",
		"fmt": "prettier -w . && xo --fix",
		"build": "rimraf dist && tsc",
		"test": "yarn build && yarn ava"
	},
	"devDependencies": {
		"@ava/typescript": "^4.0.0",
		"@lusc/tsconfig": "^2.2.0",
		"@types/node": "^20.2.1",
		"ava": "^5.2.0",
		"prettier": "^2.8.8",
		"rimraf": "^5.0.1",
		"typescript": "^5.0.4",
		"xo": "^0.54.2"
	},
	"sideEffects": false,
	"packageManager": "yarn@3.5.1"
}
