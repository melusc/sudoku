{
	"name": "@lusc/sudoku",
	"version": "5.1.1",
	"repository": "https://github.com/melusc/sudoku",
	"author": "lusc",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist/src"
	],
	"engines": {
		"node": "^18.17.1 || ^20.6.1 || ^22.0.0"
	},
	"exports": {
		".": "./dist/src/index.js",
		"./plugins": "./dist/src/plugins/index.js"
	},
	"type": "module",
	"ava": {
		"files": [
			"test/**/*.test.ts"
		],
		"environmentVariables": {
			"NODE_ENV": "test"
		},
		"typescript": {
			"compile": false,
			"rewritePaths": {
				"test/": "dist/test/"
			}
		}
	},
	"scripts": {
		"prepublishOnly": "yarn run build",
		"fmt": "prettier -w . && eslint --fix",
		"build": "rimraf dist && tsc",
		"test": "yarn build && yarn ava"
	},
	"devDependencies": {
		"@ava/typescript": "^5.0.0",
		"@lusc/eslint-config": "^3.4.2",
		"@lusc/tsconfig": "^5.1.0",
		"@types/node": "^22.10.3",
		"ava": "^6.2.0",
		"eslint": "^9.17.0",
		"prettier": "^3.4.2",
		"rimraf": "^6.0.1",
		"typescript": "^5.7.2"
	},
	"sideEffects": false,
	"packageManager": "yarn@4.6.0"
}
